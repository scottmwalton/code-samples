{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSecurityKey": "RANDOM_KEY_MUST_NOT_BE_SHARED",
  "JwtIssuer": "https://localhost",
  "JwtAudience": "https://localhost",
  "JwtExpiryInDays": 1,
  "ConnectionStrings": {
    "DefaultConnection": "Server=(local);Database=BlazorClientSideAuth;Trusted_Connection=True;MultipleActiveResultSets=true;"
  }
}
